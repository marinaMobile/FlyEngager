<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sasha_navviigazion"
    app:startDestination="@id/lauuunchFragment">

    <fragment
        android:id="@+id/lauuunchFragment"
        android:name="com.superking.parchisi.stara.one.fffrrrragg.LauuunchFragment"
        android:label="fragment_lauuunch"
        tools:layout="@layout/fragment_lauuunch" >
        <action
            android:id="@+id/action_lauuunchFragment_to_aboutEnemyAndRulesFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/aboutEnemyAndRulesFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutEnemyAndRulesFragment"
        android:name="com.superking.parchisi.stara.one.fffrrrragg.AboutEnemyAndRulesFragment"
        android:label="fragment_about_enemy_and_rules"
        tools:layout="@layout/fragment_about_enemy_and_rules" >
        <action
            android:id="@+id/action_aboutEnemyAndRulesFragment_to_gameeeFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/gameeeFragment" />
    </fragment>
    <fragment
        android:id="@+id/gameeeFragment"
        android:name="com.superking.parchisi.stara.one.fffrrrragg.GameeeFragment"
        android:label="fragment_gameee"
        tools:layout="@layout/fragment_gameee" >
        <action
            android:id="@+id/action_gameeeFragment_to_makeReloadFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/makeReloadFragment" />
        <action
            android:id="@+id/action_gameeeFragment_to_lauuunchFragment"
            app:destination="@id/lauuunchFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/lauuunchFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/makeReloadFragment"
        android:name="com.superking.parchisi.stara.one.fffrrrragg.MakeReloadFragment"
        android:label="fragment_make_reload"
        tools:layout="@layout/fragment_make_reload" >
        <action
            android:id="@+id/action_makeReloadFragment_to_lauuunchFragment"
            app:destination="@id/lauuunchFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/lauuunchFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>